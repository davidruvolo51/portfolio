    "scripts": {
        "start": "parcel serve ./src/index.html --target dev",
        "build": "parcel build ./src/index.html --target prod",
        "serve": "http-server docs/ --port 9000 -o",
        "clean": "rm -rf dist docs"
    },
    "dev": "dist/index.html",
    "prod": "docs/index.html",
    "targets": {
        "main": false,
        "dev": {
            "distDir": "dist",
            "publicUrl": ".",
            "sourceMap": false
        },
        "prod": {
            "distDir": "docs",
            "publicUrl": ".",
            "sourceMap": false
        }
    },
    "devDependencies": {
        "autoprefixer": "^9.8.6",
        "parcel": "^2.0.0-beta.1",
        "postcss-modules": "^3.2.2",
        "prop-types": "^15.7.2",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-ga": "^2.7.0",
        "react-markdown": "^4.2.2",
        "react-router-dom": "^5.1.2",
        "rss-parser": "^3.7.5"
    },
    "bugs": {
        "url": "https://davidruvolo51.github.io/portfolio/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidruvolo51/portfolio.git"
    }
