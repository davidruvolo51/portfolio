{
    "name": "my_profolio",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "start": "parcel serve ./src/index.html --target dev",
        "build": "parcel build ./src/index.html --target prod",
        "serve": "http-server docs/ --port 9000 -o",
        "clean": "rm -rf dist docs"
    },
    "targets": {
        "main": false,
        "dev": {
            "context": "browser",
            "distDir": "dist",
            "publicUrl": "."
        },
        "prod": {
            "context": "browser",
            "distDir": "docs",
            "publicUrl": ".",
            "includeNodeModules": true
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.11.2",
        "async_hooks": "^1.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.10.2",
        "react-ga": "^2.7.0",
        "rss-parser": "^3.7.5"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.1.0",
        "parcel": "^2.0.0-beta.1",
        "postcss-modules": "^3.2.2"
    },
    "bugs": {
        "url": "https://davidruvolo51.github.io/portfolio/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidruvolo51/portfolio.git"
    }
}
